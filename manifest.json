{
  "name": "WebExtensionMessageStream",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>",
    "chrome://*/*",
    "file://*/*",
    "*://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "*.html",
        "*.js",
        "*"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": ["*"]
    }
  ],
  "action": {},
  "homepage_url": "https://github.com/guest271314/WebExtensionMessageStream"
}
